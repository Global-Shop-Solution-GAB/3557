program.sub.gsfgdiesize_1_posttextmatrix.start
F.Intrinsic.Control.SetErrorHandler("gsfgdiesize_1_posttextmatrix_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.iUB.Declare(Long)
V.Local.sRet.Declare(String)
V.Local.sRow.Declare(String)


'if search mode is off we are going to go through and upate our corresponding udt recrod with the information that the user entered onto the grid

F.Intrinsic.Control.If(V.Global.bSearchActive,=,False)
	F.Intrinsic.Control.BlockEvents
		F.Intrinsic.Control.If(V.Args.Key,>,0)
			F.Intrinsic.Control.If(V.Args.Column,>,0)
				'read our flexgrid
				Gui.Form1.gsfgDieSize_1.ReadRow(V.Args.Key,V.Local.sRow)
				F.Intrinsic.Control.If(V.Local.sRow.Trim,<>,"")
					F.Intrinsic.String.Split(V.Local.sRow,"*!*",V.Local.sRow)
					F.Intrinsic.Control.If(V.Local.sRow(1).Trim,<>,"")
						'attempt to find this within our udt
						F.Intrinsic.Variable.UDTMultiSeek(V.uGlobal.uDieSizes!Die_No,V.Local.sRow(0),V.Local.sRet)
						F.Intrinsic.Control.If(V.Local.sRet.Trim,<>,"")
							F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
							V.Local.iUB.Set(V.Local.sRet(0))
						F.Intrinsic.Control.Else
							F.Intrinsic.Control.If(V.uGlobal.uDieSizes.UBound,=,-1)
								V.uGlobal.uDieSizes.Redim(0,0)
								V.Local.iUB.Set(0)
							F.Intrinsic.Control.Else
								F.Intrinsic.Math.Add(V.uGlobal.uDieSizes.UBound,1,V.Local.iUB)
								V.uGlobal.uDieSizes.RedimPreserve(0,V.Local.iUB)
							F.Intrinsic.Control.EndIf
							V.uGlobal.uDieSizes(v.Local.iUB)!Die_No.Set(V.Local.sRow(0))
						F.Intrinsic.Control.EndIf
						V.uGlobal.uDieSizes(v.Local.iUB)!Die_Size_Txt.Set(V.Local.sRow(1).Trim)
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.UnBlockEvents
F.Intrinsic.Control.Else
	F.Intrinsic.Control.BlockEvents
	F.Intrinsic.Control.CallSub(Gsfgdiesize_1_lostfocus)
	F.Intrinsic.Control.UnBlockEvents

F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("gsfgdiesize_1_posttextmatrix_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Form1_unload)
Function.Intrinsic.Control.EndIf


program.sub.gsfgdiesize_1_posttextmatrix.end

program.sub.gsfgetc_1_posttextmatrix.start
F.Intrinsic.Control.SetErrorHandler("gsfgetc_1_posttextmatrix_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sRow.Declare(String)
V.Local.sRet.Declare(String)
V.Local.iUB.Declare(Long)
V.Local.iC.Declare(String)

'this is going to update our flexgrid ETC on the Styles tab, should be first tab
'only going to fire if the row is greater than 0 (header row)
'and if column is greater than 0 (item number)
F.Intrinsic.Control.BlockEvents
F.Intrinsic.Control.If(V.Args.key,>,0)
	F.Intrinsic.Control.If(V.Args.Column,>,0)
		'first check and read the row
		Gui.Form1.gsfgETC_1.ReadRow(V.Args.key,V.Local.sRow)
		F.Intrinsic.Control.If(V.Local.sRow.Trim,<>,"")
			F.Intrinsic.String.Split(V.Local.sRow,"*!*",V.Local.sRow)
			F.Intrinsic.Control.If(V.Local.sRow(1).Trim,<>,"")
				'see if it's within our udt or not, if it is then update the corresponding record, else we are going to redim preserve our udt and add it in there for save later
				F.Intrinsic.Variable.UDTMultiSeek(V.uGlobal.uMainETC!ETC_No,V.Local.sRow(0),V.Local.sRet)
				F.Intrinsic.Control.If(V.Local.sRet.Trim,<>,"")
					F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
					V.Local.iUB.Set(V.Local.sRet(0))
				F.Intrinsic.Control.Else
					'means its not in our udt so add that thing
					F.Intrinsic.Control.If(V.uGlobal.uMainETC.UBound,=,-1)
						V.uGlobal.uMainETC.Redim(0,0)
						V.Local.iUB.Set(0)
					F.Intrinsic.Control.Else
						F.Intrinsic.Math.Add(V.uGlobal.uMainETC.UBound,1,V.Local.iUB)
						V.uGlobal.uMainETC.RedimPreserve(0,V.Local.iUB)
					F.Intrinsic.Control.EndIf
					V.uGlobal.uMainETC(v.Local.iUB)!ETC_No.Set(V.Local.sRow(0))
				F.Intrinsic.Control.EndIf
				'update our value
				V.uGlobal.uMainETC(v.Local.iUB)!ETC_Value.Set(V.Local.sRow(1).Trim)
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.Intrinsic.Control.UnBlockEvents

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("gsfgetc_1_posttextmatrix_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Form1_unload)
Function.Intrinsic.Control.EndIf


program.sub.gsfgetc_1_posttextmatrix.end

