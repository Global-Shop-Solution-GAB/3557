Program.Sub.Load_Tooling.Start
F.Intrinsic.Control.SetErrorHandler("Load_Tooling_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)
V.Local.sRouter.Declare(String)
V.Local.iub.Declare(Long)
V.Local.iToolingSeq.Declare(Long,0)

V.uGlobal.uTooling.Redim(-1,-1)

F.Intrinsic.String.Build("Select * From GCG_Styles_Tooling where Rec_ID = '{0}'",V.Global.iRecID,V.Local.sSQL)
F.ODBC.Connection!conX.OpenLocalRecordsetRO("Rst",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.conX!rst.EOF,<>,True)
	F.Intrinsic.Variable.LoadUDTFromRecordset("conX","rst","v.uglobal.uTooling",False)
F.Intrinsic.Control.Else
	F.ODBC.conX!rst.Close
	'have to find the stuff from the router on the styles tab 1 screen, then query the router_lines table to get the first 12 tooling sequences, then we are going to get the part information, then we are going to load out information into our udt tooling.
	V.Local.sRouter.Set(V.Screen.Form1!txtRouter_1.Text)
	F.Intrinsic.Control.If(V.Local.sRouter.Trim,=,"")
		F.Intrinsic.UI.Msgbox("Router on Styles tab is blank. Tooling information can not be displayed.","Router not found...")
		F.Intrinsic.Control.BlockEvents
		Gui.Form1.TabCntrl0.SetTab(0)
		F.Intrinsic.Control.UnBlockEvents
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.Else
		'have to load through our tooling sequences and load the information into our stuff. We are only loading the first ten sequences.
		F.Intrinsic.String.Build("select  Top 10(Part_WC_Outside) As Part_Wc_Outside From V_Router_Line Where Router = '{0}' And Tooling_Seq = 'Y' Order By Line_Router",V.Global.sPart_Rev.Trim,V.Local.sSQL)
		F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst",V.Local.sSQL)
		F.Intrinsic.Control.DoUntil(V.ODBC.conX!rst.EOF,=,True)
			'should be defaulted to one at initialization
			F.Intrinsic.Math.Add(V.Local.iToolingSeq,1,V.Local.iToolingSeq)
			F.Intrinsic.Control.If(V.uGlobal.uTooling.UBound,=,-1)
				V.Local.iub.Set(0)
				V.uGlobal.uTooling.Redim(0,0)
			F.Intrinsic.Control.Else
				F.Intrinsic.Math.Add(V.uGlobal.uTooling.UBound,1,V.Local.iub)
				V.uGlobal.uTooling.RedimPreserve(0,V.Local.iub)
			F.Intrinsic.Control.EndIf
			
			V.uGlobal.uTooling(v.Local.iub)!Tool_Seq.Set(V.Local.iToolingSeq)
			'open up a recordset to our other information custom GCG_INV_Style_main table
			F.Intrinsic.String.Build("Select * From GCG_INV_Style_Main Where part = '{0}'",V.ODBC.conX!rst.FieldValTrim!Part_WC_Outside,V.Local.sSQL)
			F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst1",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.conX!rst1.EOF,<>,True)
				'die no
				V.uGlobal.uTooling(v.Local.iub)!Die_NO.Set(V.ODBC.conX!rst1.FieldValTrim!Die_NO)
				'inv desc
				V.uGlobal.uTooling(v.Local.iub)!Inv_Desc.Set(V.ODBC.conX!rst1.FieldValTrim!INV_Desc)
				'pl
				V.uGlobal.uTooling(v.Local.iub)!PL.Set(V.ODBC.conX!rst1.FieldValTrim!PL)
				'sort code
				V.uGlobal.uTooling(v.Local.iub)!Sort_Code.Set(V.ODBC.conX!rst1.FieldValTrim!Sort_COde)
				'mic
				V.uGlobal.uTooling(v.Local.iub)!MIC.Set(V.ODBC.conX!rst1.FieldValTrim!MIC)
				'shrinkage
				V.uGlobal.uTooling(v.Local.iub)!Shrinkage.Set(V.ODBC.conX!rst1.FieldValTrim!Shrinkage)
				''choke depth	
				V.uGlobal.uTooling(v.Local.iub)!Choke_Depth.Set(V.ODBC.conX!rst1.FieldValTrim!Choke_Depth)
				'green press
				V.uGlobal.uTooling(v.Local.iub)!Green_Press.Set(V.ODBC.conX!rst1.FieldValTrim!Green_Press)
				'NIB_Thickness
				V.uGlobal.uTooling(v.Local.iub)!NIB_Thickness.Set(V.ODBC.conX!rst1.FieldValTrim!Nib_Thickness)
				'AdJ Press Qty
				V.uGlobal.uTooling(v.Local.iub)!A_Press_Pos.Set(V.ODBC.conx!rst1.FieldValTrim!Adj_press_QTY)
				'alt die 1 through3
				V.uglobal.uTooling(v.Local.iub)!Alt_DIE_1.Set(V.ODBC.conX!rst1.FieldValTrim!Alt_Die_1)
				V.uglobal.uTooling(v.Local.iub)!Alt_DIE_2.Set(V.ODBC.conX!rst1.FieldValTrim!Alt_Die_2)
				V.uglobal.uTooling(v.Local.iub)!Alt_DIE_3.Set(V.ODBC.conX!rst1.FieldValTrim!Alt_Die_3)
				V.uglobal.uTooling(v.Local.iub)!Alt_DIE_4.Set(V.ODBC.conX!rst1.FieldValTrim!Alt_Die_4)
			F.Intrinsic.Control.EndIf
			F.ODBC.conX!rst1.Close
			F.ODBC.conX!rst.MoveNext
		F.Intrinsic.Control.Loop
		F.ODBC.conX!rst.Close
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf

F.Intrinsic.Variable.UDTMultiQuickSort(V.uGlobal.uTooling!Tool_Seq,False)
F.Intrinsic.Control.If(V.uGlobal.uTooling.UBound,=,-1)
	V.uGlobal.uTooling.Redim(0,0)
	V.Local.iToolingSeq.Set(1)
	V.uGlobal.uTooling(0)!Tool_Seq.Set(V.Local.iToolingSeq)
F.Intrinsic.Control.Else
	F.Intrinsic.Math.Add(V.uGlobal.uTooling(v.uGlobal.uTooling.UBound)!Tool_Seq,1,V.Local.iToolingSeq)
F.Intrinsic.Control.EndIf


'check our tooling number. if it's less than ten we ahe to increment our udt to 9 because we are storing ten tooling seqs/ just in case
F.Intrinsic.Control.DoUntil(V.Local.iToolingSeq,>,10)
	'it should not be -1 -1 if it is then something majorly wrong happened.
	F.Intrinsic.Math.Add(V.uglobal.uTooling.ubound,1,V.local.iub)
	V.uGlobal.uTooling.RedimPreserve(0,V.Local.iub)
	V.uGlobal.uTooling(v.Local.iub)!Tool_Seq.Set(V.Local.iToolingSeq)
	F.Intrinsic.Math.Add(V.Local.iToolingSeq,1,V.Local.iToolingSeq)
F.Intrinsic.Control.Loop

'set our defaulted information here. RecID came from tab 1.
F.Intrinsic.Variable.UDTSetMemberValue(V.uGlobal.uTooling!Rec_ID,V.Global.iRecID)


F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("Load_Tooling_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Form1_unload)
Function.Intrinsic.Control.EndIf


Program.Sub.Load_Tooling.End

