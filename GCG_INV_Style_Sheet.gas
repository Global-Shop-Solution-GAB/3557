			f.Intrinsic.Variable.UDTToString(v.uglobal.uTooling,v.local.sUDTTitleRow(v.Local.iC).Trim,"*!*","",v.Local.sRow)
			
			F.Intrinsic.Control.If(V.Local.sRow.Trim,<>,"")
				F.Intrinsic.String.Build("{0}*!*{1}*!*{2}",V.local.sUDTTitleRow(v.local.ic).Trim,V.Local.sRowZTitle(v.Local.iC).Trim,V.Local.sRow,V.Local.sRow)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("{0}*!*{1}",V.local.sUDTTitleRow(v.local.ic).Trim,V.Local.sRowZTitle(v.Local.iC).Trim,V.Local.sRow)
			F.Intrinsic.Control.EndIf
				
			F.Intrinsic.Control.If(V.Local.sFG.Trim,=,"")
				V.Local.sFG.Set(V.Local.sRow)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("{0}!*!{1}",V.Local.sFG,V.Local.sRow,V.Local.sFG)
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.Next(V.Local.iC)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf

'F.Intrinsic.Debug.ToggleOutputListing
'F.Intrinsic.Debug.DumpVariableList

F.Intrinsic.Control.If(V.Local.sFG.Trim,<>,"")
	Gui.Form1.gsfgTooling_3.Visible(false)
	Gui.Form1.gsfgTooling_3.LoadFromString(V.local.sFG,2)
	Gui.Form1.gsfgTooling_3.Visible(True)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.UnBlockEvents

