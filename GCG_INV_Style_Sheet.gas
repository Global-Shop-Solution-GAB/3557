program.sub.cmdsave_tooling_click.start
F.Intrinsic.Control.SetErrorHandler("cmdsave_tooling_click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)
V.Local.sString.Declare(String)
V.Local.iCol.Declare(Long)
V.Local.iTSeq.Declare(Long)
V.Local.iRow.Declare(Long)
V.Local.iRows.Declare(Long)
V.Local.sValue.Declare(String)
V.Local.sRow.Declare(String)
V.Local.bInString.Declare(Boolean)
'
'
F.Intrinsic.Control.BlockEvents
'commit any changes to our grid.
Gui.Form1.gsfgTooling_3.Commit
'set our total rows, 1 less than returned rows
F.Intrinsic.Math.Sub(V.Screen.Form1!gsfgTooling_3.Rows,1,V.Local.iRows)
'WE ARE GOING TO USE THIS sString for our numeric values that we want to check and make sure that they are in fact numeric.
V.Local.sString.Set("DIE_NO*!*Stamped_NO*!*Arm_NO*!*Finger_NO")

'make sure our ubound of udt is atleast 9
F.Intrinsic.Control.If(V.uGlobal.uTooling.UBound,<,9)
	F.Intrinsic.Debug.SetLA("Something happend with our ubound. Calling Load Tooling again")
	'reload the tooling
	F.Intrinsic.Control.CallSub(Load_tooling)
F.Intrinsic.Control.EndIf

'clean up old data
F.Intrinsic.String.Build("Delete From GCG_Styles_Tooling Where Rec_ID ='{0}'",V.Global.iRecID,V.local.sSQL)
F.ODBC.Connection!conX.Execute(V.Local.sSQL)

F.Intrinsic.String.Build("Select * From GCG_Styles_Tooling Where Rec_ID = '{0}'",V.Global.iRecID,V.Local.sSQL)
F.ODBC.Connection!conX.OpenLocalRecordsetRW("rst",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.conX!rst.EOF,<>,True)
	F.Intrinsic.Debug.SetLA("An error has occured. For some reason our delete statement didn't work like it should have.")
	F.Intrinsic.UI.Msgbox("An error has occured with the databse queries. Please contact Global Shop regarding this custom work.")
	F.Intrinsic.Control.CallSub(Form1_unload)
F.Intrinsic.Control.Else
	F.Intrinsic.UI.InvokeWaitDialog("Saving...")
	'since this is the way we got to do this we are doing it this way.
'	loop through cvolumns 2 through 11 minus 1 to get the tooling seq.
	'then we are going to go through each row.
	'seems convuluted but this is what we got.
'	for every column
	F.Intrinsic.Control.For(V.Local.iCol,2,11,1)
		F.Intrinsic.Math.Sub(V.Local.iCol,1,V.Local.iTSeq)
		F.ODBC.conX!rst.AddNew
		F.ODBC.conX!rst.Set!Rec_ID(V.Global.iRecID)
		F.ODBC.conX!rst.Set!Tool_Seq(V.Local.iTSeq)
		'for every row
		F.Intrinsic.Control.For(V.Local.iRow,1,V.Local.iRows,1)
			Gui.Form1.gsfgTooling_3.ReadRow(V.Local.iRow,V.Local.sRow)
			F.Intrinsic.Control.If(V.Local.sRow.Trim,<>,"")
				F.Intrinsic.String.Split(V.Local.sRow,"*!*",V.Local.sRow)
				'check if the field we are (v.local.sRow(0)) requires a numeric value (sString delimited to show our required numbered fileds)
				'ucase the compare, becuase The True,False boolean appears busted
				F.Intrinsic.String.IsInString(V.Local.sString,V.Local.sRow(0),True,V.Local.bInString)
				F.Intrinsic.Control.If(V.Local.bInString,=,True)
					F.Intrinsic.Control.CallSub(Isnumber,"Input",V.Local.sRow(v.Local.iCol))
					V.Local.sValue.Set(V.Args.Output)
				F.Intrinsic.Control.Else
					V.Local.sValue.Set(V.Local.sRow(v.Local.iCol))
				F.Intrinsic.Control.EndIf
				'so somehow a ! and * are coming in here
				F.Intrinsic.String.Replace(V.Local.sRow(0).Trim,"!","",V.Local.sRow(0).Trim)
				F.Intrinsic.String.Replace(V.Local.sRow(0).Trim,"*","",V.Local.sRow(0).Trim)
				F.Intrinsic.Control.If(V.Local.sRow(0).Trim,=,"PL")
					F.Intrinsic.String.Left(V.Local.sValue,2,V.Local.sValue)
				F.Intrinsic.Control.EndIf
				'we are really only worried about the specific column ordinal of our row for the data that we are pulling
				'item for the dtabase being in the first column (0), tooling seq based on column, and col value is our update value
				F.ODBC.conX!rst.Set![v.local.sRow(0)](v.local.sValue.Trim)
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.Next(V.Local.iRow)
	F.Intrinsic.Control.Next(V.Local.iCol)
	F.ODBC.conX!rst.Update
	F.ODBC.conX!rst.Close
	F.Intrinsic.UI.CloseWaitDialog
	F.Intrinsic.UI.Msgbox("Tooling has been saved!")
F.Intrinsic.Control.EndIf



F.Intrinsic.Control.UnBlockEvents

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdsave_tooling_click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Form1_unload)
Function.Intrinsic.Control.EndIf


program.sub.cmdsave_tooling_click.end

program.sub.cmdsave_qlty_click.start
F.Intrinsic.Control.SetErrorHandler("cmdsave_qlty_click_Err")
F.Intrinsic.Control.ClearErrors

'this sub fires when they click the save on the quality plan maintenance screen.
'we are saving all our qlty udt's to their corresponding tables.
'we are then saving our checkbox values also.
V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)
