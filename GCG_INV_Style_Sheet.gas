	F.Intrinsic.Control.If(V.uGlobal.uSearchItems.UBound,>,-1)
		F.Intrinsic.Variable.UDTMultiFlagDuplicates(V.uGlobal.uSearchItems!Control)
		F.Intrinsic.Variable.UDTDeleteFlagged(V.uGlobal.uSearchItems)
		F.Intrinsic.String.Split(V.uGlobal.uSearchItems(0)!Control,"*!*",V.Local.sControl)
		F.Intrinsic.String.Split(V.uGlobal.uSearchItems(0)!TableField,"*!*",V.Local.sTableFIeld)
		F.Intrinsic.String.Split(V.uGlobal.uSearchItems(0)!Value,"*!*",V.Local.sVal)
		V.Local.iUB.Set(V.Local.sControl.UBound)
		F.Intrinsic.Control.If(V.Local.sTableFIeld.UBound,<>,V.Local.iUB)
			F.Intrinsic.Debug.SetLA("ubound of table field did not match our other ubounds... can't search")
			F.Intrinsic.Control.exitsub
		F.Intrinsic.Control.ElseIf(V.Local.sVal.UBound,<>,V.Local.iUB)
			F.Intrinsic.Debug.SetLA("ubound of values did not match our other ubounds...cant search")
			F.Intrinsic.Control.ExitSub
		F.Intrinsic.Control.Else
			'build or string, it can be rather large so be prepared
			F.Intrinsic.Control.For(V.Local.iC,0,V.Local.iUb,1)
				F.Intrinsic.Control.If(V.Local.sVal(v.Local.iC).Trim,<>,"")
					'have to replace any single ticks with 2 ticks
					F.Intrinsic.String.Replace(V.Local.sVal(v.Local.iC),"'","''",V.Local.sVal(v.Local.iC))
					F.Intrinsic.Control.If(V.Local.sControl(v.Local.iC).Trim,=,"gsfgDieSize_1")
						'going to build or string for die size information.
						F.Intrinsic.Control.If(V.Local.sSQLDie.Trim,=,"")
							F.Intrinsic.String.Build("Select * From GCG_INV_Die_Sizes Where Die_Size_TXT Like '%{0}%'",V.Local.sVal(v.Local.iC).Trim,V.Local.sSQLDie)
						F.Intrinsic.Control.Else
							F.Intrinsic.String.Build("{0} And Die_Size_Txt Like '%{1}%'",V.Local.sSQLDie,V.Local.sVal(v.Local.iC).Trim,V.Local.sSQLDie)
							F.Intrinsic.String.Build("{0} And Die_Size Like '%{1}%'",V.Local.sUPLSQL,V.Local.sVal(v.Local.iC).Trim,V.Local.sUPLSQL)
						F.Intrinsic.Control.EndIf
						F.Intrinsic.Control.If(V.Local.sUPLSQL.Trim,=,"")
							F.Intrinsic.String.Build("Select * From GCG_Styles_Upl_Data Where Die_Size  Like '%{0}%'",V.Local.sVal(v.Local.iC).Trim,V.Local.sUPLSQL)
						F.Intrinsic.Control.Else
							F.Intrinsic.String.Build("{0} And Die_Size Like '%{1}%'",V.Local.sUPLSQL,V.Local.sVal(v.Local.iC).Trim,V.Local.sUPLSQL)
						F.Intrinsic.Control.EndIf
					F.Intrinsic.Control.Else
						F.Intrinsic.Control.If(V.Local.sSQLMain.Trim,=,"")
							F.Intrinsic.String.Build("Select * From GCG_INV_Style_Main Where {0} Like '%{1}%'",V.Local.sTableFIeld(v.Local.iC).Trim,V.Local.sVal(v.Local.iC).Trim,V.Local.sSQLMain)
						F.Intrinsic.Control.Else
							F.Intrinsic.String.Build("{0} And {1} Like '%{2}%'",V.Local.sSQLMain,V.Local.sTableFIeld(v.Local.iC).Trim,V.Local.sVal(v.Local.iC).Trim,V.Local.sSQLMain)
						F.Intrinsic.Control.EndIf
						'uploda table doesnt haave cust cross or part cross reference filds.
						F.Intrinsic.Control.If(V.Local.sTableFIeld(v.Local.iC).UCASE,<>,"PART_X_REF")
							F.Intrinsic.Control.If(V.Local.sTableFIeld(V.Local.iC).uCase,<>,"CUST_X_REF")
								F.Intrinsic.Control.If(V.Local.sUPLSQL.Trim,=,"")
									F.Intrinsic.String.Build("Select * From GCG_Styles_Upl_Data Where {0}  Like '%{1}%'",V.Local.sTableFIeld(v.Local.iC).Trim,V.Local.sVal(v.Local.iC).Trim,V.Local.sUPLSQL)
								F.Intrinsic.Control.Else
									F.Intrinsic.String.Build("{0} And {1} Like '%{2}%'",V.Local.sUPLSQL,V.Local.sTableFIeld(v.Local.iC).Trim,V.Local.sVal(v.Local.iC).Trim,V.Local.sUPLSQL)
								F.Intrinsic.Control.Endif
							F.Intrinsic.Control.EndIf
						F.Intrinsic.Control.EndIf
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.Next(V.Local.iC)
		F.Intrinsic.Control.EndIf
		'open up our recordset
		F.Intrinsic.Control.If(V.Local.sSQLDie.Trim,<>,"")
			F.ODBC.Connection!conX.OpenLocalRecordsetRO("rstDie",V.Local.sSQLDie)
			F.Intrinsic.Variable.LoadUDTFromRecordset("conX","rstDie","v.uglobal.uDieSizes",False,100)
			F.ODBC.conX!rstDie.Close
		F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.If(V.Local.sSQLMain.Trim,<>,"")
			F.ODBC.Connection!conX.OpenLocalRecordsetRO("rstMain",V.Local.sSQLMain)
			F.Intrinsic.Variable.LoadUDTFromRecordset("conX","rstMain","v.uglobal.uMain",False,500)
			F.ODBC.conX!rstMain.Close
		F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.If(V.Local.sUPLSQL.Trim,<>,"")
			F.ODBC.Connection!conX.OpenLocalRecordsetRO("rstUPL",V.Local.sUPLSQL)
			F.Intrinsic.Control.If(V.ODBC.conX!rstUpl.EOF,<>,True)
				F.Intrinsic.Variable.LoadUDTFromRecordset("conx","rstupl","v.uglobal.uUPLData",False,100)
				F.Intrinsic.Variable.UDTSetMemberValue(V.uGlobal.uUPLData!Rec_ID,-1)
			F.Intrinsic.Control.EndIf
		F.ODBC.conX!rstupl.Close
		F.Intrinsic.Control.EndIf



		F.Intrinsic.Control.If(V.uGlobal.uUPLData.UBound,>,-1)
			F.Intrinsic.Control.If(V.uGlobal.uMain.UBound,>,-1)
				F.Intrinsic.Variable.UDTCopy(V.uGlobal.uUPLData,V.uGlobal.uMain,True)
			F.Intrinsic.Control.Else
				F.Intrinsic.Variable.UDTCopy(V.uGlobal.uUPLData,V.uGlobal.uMain,False)
			F.Intrinsic.Control.EndIf

		F.Intrinsic.Control.EndIf

		'prepare our record search
		V.Local.iC.Set(V.uGlobal.uMain.UBound)
		F.Intrinsic.Control.If(V.uGlobal.uMain.UBound,>=,0)
			F.Intrinsic.String.Build("{0} of {1} Records Found.",1,V.Local.iC,V.Local.sVal)
			Gui.Form1.lblNumRecords.Caption(V.Local.sVal)
			'disbaled on load, only enable them if search mode has been executed and performed.
			Gui.Form1.cmdPrev_1.Enabled(True)
			Gui.Form1.cmdNext_1.Enabled(True)
			Gui.Form1.cmdNext_2.Enabled(True)
			Gui.Form1.cmdPrev_2.Enabled(True)
			'disable search mode
			F.Intrinsic.Control.CallSub(Cmdsearchmode_click)
			F.Intrinsic.Control.CallSub(Load_screen_1)
			'have to put this here because cmdsearchmode_Click disblaes search mode and hides the num label ...
			Gui.Form1.cmdNext_1.SetFocus
			Gui.Form1.lblNumRecords.Visible(True)
		F.Intrinsic.Control.Else
			Gui.Form1.lblNumRecords.Caption("No records found!")
			Gui.Form1.lblNumRecords.Visible(True)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
	F.Intrinsic.UI.CloseWaitDialog
	V.uGlobal.uSearchItems.Redim(-1,-1)
	F.Intrinsic.Control.UnBlockEvents
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("keypressenter_search_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
