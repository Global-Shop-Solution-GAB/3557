V.Local.iETCID.Declare(Long)

'qlty plan main save table
F.Intrinsic.Control.BlockEvents
F.Intrinsic.UI.InvokeWaitDialog("Saving Quality Information...")
'determine if we have a qlty plan main record with the corresponding rec i d
F.Intrinsic.Control.If(V.uGlobal.uQltyMain.UBound,=,-1)
	V.uGlobal.uQltyMain.Redim(0,0)
	V.uGlobal.uQltyMain(0)!Rec_ID.Set(V.Global.iRecID)
	V.uGlobal.uQltyMain(0)!In_Catalog.Set(V.Screen.Form1!chkInCatalog_2.Value)
