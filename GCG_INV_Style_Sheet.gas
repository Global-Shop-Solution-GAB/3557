F.Intrinsic.Control.BlockEvents
Gui.Form1.frmTab2.Visible(false)

F.Intrinsic.Control.If(V.Global.iRecID,=,-1)
	'call this to save our information on the screen from the other one.
	F.Intrinsic.Control.CallSub(Cmdsave_style_click)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.If(V.Global.iRecID,<>,-1)
	F.Intrinsic.Control.CallSub(Load_quality)
	'now load our screen information
	F.Intrinsic.Control.If(V.uGlobal.uQltyPlan.UBound,<>,-1)
		F.Intrinsic.Variable.UDTMultiQuickSort(V.uGlobal.uQltyPlan!Qlty_Plan_NO,False)
		Gui.Form1.gsfgQualityNames_2.LoadFromUDT("V.UGLOBAL.uQltyPlan","Qlty_Plan_NO*!*Qlty_Plan_Name",2)
		F.Intrinsic.Math.Sub(V.Screen.Form1!gsfgQualityNames_2.Rows,1,V.Local.iC)
		Gui.Form1.txtMaxQtyPlans_2.Text(V.Local.iC)
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.If(V.uGlobal.uQltyAttr.UBound,<>,-1)
		Gui.Form1.gsfgQltyAttributes_2.LoadFromUDT("v.uglobal.uQltyAttr","Attribute_Name*!*Nominal*!*High_Limit*!*Low_Limit*!*Freq*!*Method*!*Comment*!*Qlty_Plan_Num",2)
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Variable.UDTMultiQuickSort(V.uGlobal.uQltyEtc!ETC_No,False)
	F.Intrinsic.Control.If(V.uGlobal.uQltyEtc.UBound,<>,-1)
		Gui.Form1.gsfgEtc_2.LoadFromUDT("V.UGLOBAL.uQltyEtc","ETC_NO*!*ETC_Value",2)
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Math.Sub(V.Screen.Form1!gsfgEtc_2.Rows,1,V.Local.iRows)
	Gui.Form1.gsfgEtc_2.Rows(251)
	F.Intrinsic.Control.For(V.Local.iRow,V.Local.iRows,250,1)
		V.Local.iNum.Set(V.Local.iRow)
		Gui.Form1.gsfgEtc_2.TextMatrix(0,V.Local.iRow,V.Local.iNum)
		Gui.Form1.gsfgEtc_2.TextMatrix(1,V.Local.iRow,"")
		Gui.Form1.gsfgEtc_2.ApplyStyle(V.Local.iRow,2)
	F.Intrinsic.Control.Next(V.Local.iRow)



