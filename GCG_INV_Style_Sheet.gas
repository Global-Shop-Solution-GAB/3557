program.sub.gsfgmisc_2_posttextmatrix.start
F.Intrinsic.Control.SetErrorHandler("gsfgmisc_2_posttextmatrix_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sRow.Declare(String)
V.Local.iC.Declare(String)
V.Local.sRet.Declare(String)
V.Local.iUB.Declare(Long)

F.Intrinsic.Control.BlockEvents
'here we are going to update our misc udt to the flexgrid within our quality tab or screen.
F.Intrinsic.Control.If(V.Args.Key,>,0)
	F.Intrinsic.Control.If(V.Args.column,>,0)
		'read our row, maybe, becuase our key should be our misc number? which it is.
		Gui.Form1.gsfgMisc_2.ReadRow(V.Args.Key,V.Local.sRow)
		F.Intrinsic.Control.If(V.Local.sRow.Trim,<>,"")
			F.Intrinsic.String.Split(V.Local.srow,"*!*",V.Local.sRow)
			F.Intrinsic.Variable.UDTMultiSeek(V.uGlobal.uQltyMISC!Misc_No,V.Local.sRow(0).Trim,V.Local.sRet)
			F.Intrinsic.Control.If(V.Local.sRet.Trim,<>,"")
				'and update our udt.
				F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
				V.Local.iUB.Set(V.Local.sRet(0))
				V.uGlobal.uQltyMISC(v.Local.iUB)!MISC_No.Set(V.Local.sRow(0).Trim)
				V.uGlobal.uQltyMISC(v.Local.iUB)!MISC_Value.set(V.Local.sRow(1).Trim)
			F.Intrinsic.Control.Else
				F.Intrinsic.Debug.SetLA("UBound: ",V.uGlobal.uQltyMISC.UBound," Lower Bound: ",V.uGlobal.uQltyMisc.LBound)
				'we know we have to increment and add out our udt
				F.Intrinsic.Control.If(V.uGlobal.uQltyMisc.UBound,=,-1)
					V.Local.iUB.Set(0)
					V.uGlobal.uQltyMisc.Redim(0,V.Local.iUB)
				F.Intrinsic.Control.Else
					F.Intrinsic.Math.Add(V.uGlobal.uQltyMisc.UBound,1,V.Local.iUB)
					V.uGlobal.uQltyMISC.RedimPreserve(0,V.Local.iUB)
				F.Intrinsic.Control.EndIf
				V.uGlobal.uQltyMISC(v.Local.iUB)!MISC_No.Set(V.Local.sRow(0).Trim)
			F.Intrinsic.Control.Endif
			V.uGlobal.uQltyMISC(v.Local.iUB)!MISC_Value.set(V.Local.sRow(1).Trim)
		F.Intrinsic.Control.EndIf
	F.INtrinsic.Control.EndIf
F.Intrinsic.Control.EndIf


F.Intrinsic.Control.UnBlockEvents

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("gsfgmisc_2_posttextmatrix_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Form1_unload)
