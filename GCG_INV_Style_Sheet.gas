program.sub.gsfgqltyattributes_2_posttextmatrix.start
F.Intrinsic.Control.SetErrorHandler("gsfgqltyattributes_2_posttextmatrix_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sRet.Declare(String)
V.Local.sRow.Declare(String)
V.Local.iC.Declare(Long)


'this will update the information on the grid one by one as they are typing and tabbing through,
'just becuase the amount of time it would take to either run through the entire flexgrid when they only change 1 or 2.
'and also, becuase at this time using export to udt function doesn't allow you to use udtsetmembervalue.
F.Intrinsic.Control.BlockEvents
F.Intrinsic.Control.If(V.Args.key,>,0)
	F.Intrinsic.Control.If(V.Args.column,>,0)
		Gui.Form1.gsfgQltyAttributes_2.ReadRow(V.Args.key,V.Local.sRow)
		F.Intrinsic.Control.If(V.Local.sRow.Trim,<>,"")
			F.Intrinsic.String.Split(V.Local.sRow,"*!*",V.Local.sRow)
			F.Intrinsic.Variable.UDTMultiSeek(V.uGlobal.uQltyAttr!Attribute_Name,V.Local.sRow(0).Trim,V.Local.sRet)
			F.Intrinsic.Control.If(V.Local.sRet.Trim,<>,"")
				'set the column args to ic
				V.Local.iC.Set(V.Args.column)
				F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
				'only going to worry about the first return.  and update our udt
				F.Intrinsic.Control.SelectCase(V.Local.iC)
					F.Intrinsic.Control.Case(1)
						'nominal column
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!Nominal.Set(V.Local.sRow(v.Local.iC))
					F.Intrinsic.Control.Case(2)
						'high limit column
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!High_Limit.Set(V.Local.sRow(v.Local.iC))
					F.Intrinsic.Control.Case(3)
						'low limit column
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!Low_Limit.Set(V.Local.sRow(v.Local.iC))
					F.Intrinsic.Control.Case(4)
						'frequency column
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!Freq.Set(V.Local.sRow(v.Local.iC))
					F.Intrinsic.Control.Case(5)
						'method column
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!Method.Set(V.Local.sRow(v.Local.iC))
					F.Intrinsic.Control.Case(6)
						'comment column
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!Comment.Set(V.Local.sRow(v.Local.iC))
					F.Intrinsic.Control.Case(7)
						'quality plan no
						V.uGlobal.uQltyAttr(v.Local.sRet(0))!Qlty_Plan_Num.Set(V.Local.sRow(v.Local.iC))
				F.Intrinsic.Control.EndSelect
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.Intrinsic.Control.UnBlockEvents


F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("gsfgqltyattributes_2_posttextmatrix_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_INV_Style_Sheet.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Form1_unload)
Function.Intrinsic.Control.EndIf


