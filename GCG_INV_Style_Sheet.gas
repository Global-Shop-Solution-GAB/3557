program.sub.gsfgdiesize_1_lostfocus.start
F.Intrinsic.Control.SetErrorHandler("gsfgdiesize_1_lostfocus_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.iC.Declare(Long)
V.Local.iUB.Declare(Long)
V.Local.sVal.Declare(String)
F.Intrinsic.Control.If(V.Global.bSearchActive,=,True)
	F.Intrinsic.Control.BlockEvents
	Gui.Form1.gsfgDieSize_1.Commit
'	search mode is on, and they neted something into the die size udt. possibly we are going to loop through add add some records to our udt
	'its either going to be first occurence or new occurence. THen what we are going to do is control name is gsfgDieSize_1, then the value, then the table name is stored in the meta data 0
	F.Intrinsic.Control.For(V.Local.iC,1,20,1)
		Gui.Form1.gsfgDieSize_1.GetTextMatrix(1,V.Local.iC,V.Local.sVal)
		F.Intrinsic.Control.If(V.Local.sVal.Trim,<>,"")
			F.Intrinsic.Control.If(V.uGlobal.uSearchItems.UBound,=,-1)
				V.uGlobal.uSearchItems.Redim(0,0)
				V.uGlobal.uSearchItems(0)!Control.Set("gsfgDieSize_1")
				V.uGlobal.uSearchItems(0)!TableField.Set(V.screen.Form1!gsfgDieSize_1.MetaData0)
				V.uGlobal.uSearchItems(0)!Value.set(V.local.sVal.Trim)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("{0}*!*{1}",V.uGlobal.uSearchItems(0)!Control,"gsfgDieSize_1",V.uGlobal.uSearchItems(0)!Control)
				F.Intrinsic.String.Build("{0}*!*{1}",V.uGlobal.uSearchItems(0)!TableField,V.Screen.Form1!gsfgDieSize_1.MetaData0,V.uGlobal.uSearchItems(0)!TableField)
				F.Intrinsic.String.Build("{0}*!*{1}",V.uGlobal.uSearchItems(0)!Value,V.local.sVal.Trim,V.uGlobal.uSearchItems(0)!Value)
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(V.Local.iC)
	F.Intrinsic.Control.UnBlockEvents
F.Intrinsic.Control.EndIf
F.Intrinsic.Control.ExitSub

