	V.uGlobal.uQltyMain(0)!Leave_Out_Catalog.Set(V.Screen.Form1!chkLeavOutCat_2.Value)
	V.uGlobal.uQltyMain(0)!Add_To_Catalog.Set(V.Screen.Form1!chkAddtoCat_2.Value)
	f.ODBC.Connection!conX.GetID("GCG_Styles_Qlty_Plan","Qlty_Plan_ID",True,v.Local.iPlanID)
	f.ODBC.Connection!conX.getid("GCG_Styles_Qlty_Misc","Qlty_Plan_Misc_ID",True,v.Local.iMiscID)
	f.ODBC.Connection!conX.getid("GCG_Styles_Qlty_Etc","Qlty_Plan_ETC_ID",True,v.Local.iETCID)
