Program.Sub.Load_Job.Start
F.Intrinsic.Control.SetErrorHandler("Load_Job_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)
V.Local.sT.Declare(String)
V.Local.iRet.Declare(String)
V.Local.sPart.Declare(String)
V.Local.sRows.Declare(String)
V.Local.iRows.Declare(Long)
V.Local.sRet.Declare(String)

V.Local.iC.Declare(Long)

'get fg part from table
F.Intrinsic.String.Build("Select Part From V_JOB_HEADER WHERE Job = '{0}' And Suffix = '{1}'",V.Global.sJob.Trim,V.Global.sSuffix.Trim,V.Local.sSQL)
F.ODBC.Connection!conX.OpenLocalRecordsetRO("Rst",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.conX!Rst.EOF,<>,True)
	V.local.sPart.set(V.ODBC.conX!Rst.FieldValTrim!Part)
F.Intrinsic.Control.EndIf
F.ODBC.conX!Rst.Close

F.Intrinsic.Control.if(V.Local.sPart.Trim,<>,"")
	'grab our Rec_ID from the Record and get to our QLty+Plan_ID.
	F.Intrinsic.String.Build("Select Rec_ID From GCG_INV_Style_Main Where Part = '{0}'",V.local.sPart,V.Local.sSQL)
	F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.conX!rst.EOF,<>,True)
		F.Intrinsic.String.Build("Select Qlty_Plan_ID From GCG_Styles_Qlty_Main Where Rec_ID = '{0}'",V.ODBC.conX!Rst.FieldValLong!Rec_ID,V.Local.sSQL)
		F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst1",V.Local.sSQL)
		F.Intrinsic.Control.If(V.ODBC.conX!Rst1.EOF,<>,True)
			'grab our Qlty_Plan_ID and get some other information from our Qlty_Plan_Attr table. key being Qlty_PLan_ID
			F.Intrinsic.String.Build("Select Qlty_Plan_REC_ID, Attribute_Name,Nominal,High_Limit,Low_Limit From GCG_Styles_Qlty_Attr Where Qlty_Plan_REC_ID = '{0}'",V.ODBC.conX!rst1.FieldValLong!Qlty_Plan_ID,V.Local.sSQL)
			'have to have specific order by.....
			F.Intrinsic.String.Build("{0} {1}",V.Local.sSQL," Order by Case When Attribute_Name = 'THK' Then 0 Else 1 End,Case When Attribute_Name = 'IC' Then 1 Else 2 End, Case When Attribute_Name = 'WD'Then 2 Else 3 End,Case When Attribute_Name = 'LG' Then 3 Else 4 End,Case When Attribute_Name='HOLE' Then 4 Else 5 End, Case When Attribute_Name='C''SINK' Then 5 Else 6 End,Case When Attribute_Name='C''BORE' Then 6 Else 7 End, Case When Attribute_Name='BALLDIM' Then 8 Else 9 End,Case When Attribute_Name='Clear' Then 9 Else 10 End,Case When Attribute_Name='Rad' Then 10 Else 11 End,Case When Attribute_Name='CHAMF' Then 11 Else 12 End,Case When Attribute_Name='Finished Press Flat' Then 12 Else 13 End",V.Local.sSQL)
			F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst2",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.conX!rst2.EOF,<>,True)
				'load our udt from recordset.
				F.Intrinsic.Variable.LoadUDTFromRecordset("ConX","rst2","v.uglobal.uQltyAttr",False)
				'due to the way we want to display the data we need to load it a bit differently, we want the attribute information to be done in a specific way.
				'load our flexgrid right after it.	
				Gui.Form1.gsfgLimits.LoadFromUDT("v.uglobal.uQltyAttr","Attribute_Name*!*Nominal*!*High_Limit*!*Low_Limit",2)
			F.Intrinsic.Control.EndIf
			F.ODBC.conX!rst2.Close
		F.Intrinsic.Control.EndIf
		F.ODBC.conX!rst1.Close
	F.Intrinsic.Control.EndIf
	F.ODBC.conX!Rst.Close
F.Intrinsic.Control.EndIf



F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("Load_Job_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_Qlty_Items.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Unload)
Function.Intrinsic.Control.EndIf


Program.Sub.Load_Job.End

Program.Sub.Load_Hist.Start
F.Intrinsic.Control.SetErrorHandler("Load_Hist_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)

'get our latest hist id
F.Intrinsic.String.Build("Select Top 1 Hist_Seq From GCG_WO_Qlty_Hist Where Job ='{0}' And Suffix = '{1}' Order By Hist_Seq DESC",V.Global.sJob.Trim,V.Global.sSuffix.Trim,V.Local.sSQL)
F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.conX!Rst.EOF,<>,True)
	'grab our last hist id
	V.Global.iHistSeq.Set(V.ODBC.conX!Rst.FieldValLong!Hist_Seq)
	F.ODBC.conX!Rst.Close
	F.Intrinsic.Control.If(V.Global.bGodMode,=,False)
		F.Intrinsic.String.Build("Select Rec_ID, Attribute_Name,Hist_Seq,Val_1,Val_2,Val_3,Val_4,Val_5 From GCG_WO_Qlty_Hist Where Job ='{0}' And Suffix = '{1}' And Hist_Seq = '{2}' Order By Rec_ID",V.Global.sJob.Trim,V.global.ssuffix.trim,V.Global.iHistSeq,V.Local.sSQL)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Build("Select Rec_ID, Attribute_Name,Hist_Seq,Val_1,Val_2,Val_3,Val_4,Val_5 From GCG_WO_Qlty_Hist Where Job ='{0}' And Suffix = '{1}' Order By Rec_ID",V.Global.sJob.Trim,V.global.ssuffix.trim,V.Local.sSQL)
	F.Intrinsic.Control.EndIf
	F.ODBC.Connection!conX.OpenLocalRecordsetRO("Rst",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.conX!Rst.EOF,<>,True)
		F.Intrinsic.Variable.LoadUDTFromRecordset("conX","rst","v.uglobal.uQltyHist",False)
		'look for the greatest history number. cause we are loading that one to our grid.
'		F.Intrinsic.Variable.UDTMultiQuickSort(V.uGlobal.uQltyHist!Hist_Seq,True)
		'change FINISHED_PRESS_FLAT TO NOT HAVE UNDERSCORes	
		F.Intrinsic.Control.If(V.Global.bGodMode,=,False)
			F.Intrinsic.Variable.UDTMultiSeekSet(V.uGlobal.uQltyHist!Attribute_Name,"FINISHED_PRESS_FLAT",V.uGlobal.uQltyHist!Attribute_Name,"FINISHED PRESS FLAT")
			'load our flexgrid from out first element only since this is the most recent history that we want to see.
			Gui.Form1.gsfgHist.LoadFromUDT("v.uglobal.uQltyHIst","Attribute_Name*!*Val_1*!*Val_2*!*Val_3*!*Val_4*!*Val_5",2)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.ODBC.conX!Rst.Close

'if we are in god mode load our distinct information into our uHistHead udt.
F.Intrinsic.Control.If(V.Global.bGodMode,=,True)
	F.Intrinsic.String.Build("Select Distinct Hist_Seq, User_entered,Date_Entered From GCG_WO_Qlty_Hist Where Job = '{0}' And Suffix = '{1}'",V.Global.sJob.Trim,V.Global.sSuffix.Trim,V.Local.sSQL)
	F.ODBC.Connection!conX.OpenLocalRecordsetRO("rst",V.Local.sSQL)
