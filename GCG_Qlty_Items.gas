program.sub.cmdsave_click.start
F.Intrinsic.Control.SetErrorHandler("cmdsave_click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sBox.Declare(String)
V.Local.iB.Declare(Long)
V.Local.iItem.Declare(Long)
V.Local.fNum.Declare(Float)
V.Local.sSQL.Declare(String)
V.Local.iRec.Declare(Long)

F.Intrinsic.Control.BlockEvents
'F.Intrinsic.String.Split("THK*!*CBORE*!*IC*!*BALLDIM*!*WD*!*CLEAR*!*LG*!*RAD*!*HOLE*!*CHAMF*!*CSINK*!*FIN","*!*",V.Local.sBox)
F.Intrinsic.String.Split("THK*!*IC*!*WD*!*LG*!*HOLE*!*CSINK*!*CBORE*!*BALLDIM*!*CLEAR*!*RAD*!*CHAMF*!*FINISHED_PRESS_FLAT","*!*",V.Local.sBox)

'incement our history global iHistSeq variable, as this should be the last history sequence that we had. We can have as many as we want .
'if we are not in god mode cause this is a new entry and we need to add a new record.
F.Intrinsic.Control.If(V.Global.bGodMode,=,False)
	F.Intrinsic.Control.If(V.Global.iHistSeq,=,0)
		V.Global.iHistSeq.Set(1)
	F.Intrinsic.Control.Else
		F.Intrinsic.math.Add(V.Global.iHistSeq,1,V.Global.iHistSeq)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
'if god mode is true we are going to remove all records then carry on as if they are new but not really.
F.Intrinsic.Control.If(V.Global.bGodMode,=,True)
	F.Intrinsic.UI.InvokeWaitDialog("Saving...")
	Gui.Form1..Visible(False)
	F.Intrinsic.String.Build("Delete From GCG_WO_Qlty_Hist Where Job = '{0}' And Suffix = '{1}' And Hist_seq = '{2}'",V.Global.sJob,V.Global.sSuffix,V.Global.iHistSeq,V.Local.sSQL)
	F.ODBC.Connection!conX.Execute(V.Local.sSQL)
	Gui.Form1.gsfgHist.Enabled(True)
	Gui.Form1.cmdSave.Enabled(False)
F.Intrinsic.Control.EndIf

'now we should be good to go.
F.Intrinsic.String.Build("Select * From GCG_WO_Qlty_Hist Where Job = '{0}' And Suffix = '{1}' And Hist_seq = '{2}'",V.Global.sJob,V.Global.sSuffix,V.Global.iHistSeq,V.Local.sSQL)
F.ODBC.Connection!conX.OpenLocalRecordsetRW("rst",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.conX!Rst.EOF,=,False)
	'trythisandseeifitworks.Weshouldcallthissubagainandintheoryitshouldtrytoincrementthenexton
	F.Intrinsic.Control.CallSub(Cmdsave_click)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.For(V.Local.iB,0,V.Local.sBox.UBound,1)
	'get id for our field, new id for each record
	f.ODBC.Connection!conX.GetID("GCG_WO_Qlty_Hist","Rec_ID",True,v.Local.iRec)
	F.ODBC.conX!Rst.AddNew
	F.ODBC.conX!Rst.Set!Rec_ID(V.Local.iRec)
	F.ODBC.conX!Rst.Set!Job(V.Global.sJob.triM)
	F.ODBC.conX!Rst.Set!Suffix(V.Global.sSuffix.Trim)
	F.ODBC.conX!Rst.Set!Hist_Seq(V.Global.iHistSeq)
	F.ODBC.conX!Rst.Set!Attribute_Name(V.Local.sBox(v.Local.iB))
	F.Intrinsic.Control.If(V.Global.bGodMode,=,False)
		'employee on the job
		F.ODBC.conX!Rst.Set!user_entered(V.passed.009993)
	F.Intrinsic.Control.Else
		F.ODBC.conX!Rst.Set!user_entered(V.caller.user)
	F.Intrinsic.Control.EndIf
	f.ODBC.conX!Rst.Set!Date_Entered(v.Ambient.Date.PervasiveDate)
	F.Intrinsic.Control.For(V.Local.iItem,1,5,1)
		F.Intrinsic.Debug.SetLA("Attribute: ",V.Local.sBox(v.Local.iB))
		F.Intrinsic.Debug.SetLA(" Val: ",V.Local.iItem)
		'check each one, we are starting with this, any that are blank or non numeric they will be defaulted to numeric.
		'I wonder it it is faster to write to udt then to recordset? or just stright to recordset?
		f.Intrinsic.Variable.SetNumeric(v.Screen.Form1!txt_[v.local.sBox(v.local.iB)]_[v.local.iItem].Text,0,v.Local.fNum)
		F.Intrinsic.Debug.SetLA("saving this Number. ",V.local.fNum)
'		F.Intrinsic.Control.If(V.Local.fNum,<>,0)
''			F.Intrinsic.String.Format(V.Local.fNum,"##########.######",V.Local.fNum)
'		F.Intrinsic.Control.EndIf
		F.Intrinsic.Debug.SetLA("Formatted Number: ",V.Local.fNum)
		F.ODBC.conX!Rst.Set!Val_[v.local.iItem](V.Local.fNum)
	F.Intrinsic.Control.Next(V.Local.iItem)
	F.ODBC.conX!Rst.Update
F.Intrinsic.Control.Next(V.Local.iB)
F.ODBC.conX!Rst.Close

'if hook is on gui, end our form.
F.Intrinsic.Control.If(V.Caller.Hook,=,"15880")
	F.Intrinsic.Control.CallSub(Unload)
F.Intrinsic.Control.Else
 	'we need to clear our textboxes and let the user continue on the main scree500004n.
	F.Intrinsic.Control.CallSub(Clear_txt)
	'reload our history
	F.Intrinsic.Control.CallSub(Load_hist)
	F.Intrinsic.UI.CloseWaitDialog
	Gui.Form1.cmdClear.Enabled(False)
	Gui.Form1.cmdClear.Visible(False)
	Gui.Form1.lblCaption.Caption("")
	Gui.Form1..Visible(True)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.UnBlockEvents
F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdsave_click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_Qlty_Items.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Unload)
Function.Intrinsic.Control.EndIf


